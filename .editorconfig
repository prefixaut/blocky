# For documentation see: http://editorconfig.org

root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

[*.scss]
indent_style = space
indent_size = 2

[*.html]
indent_style = space
indent_size = 4

[{*.js,*.json}]
indent_style = space
indent_size = 4

[*.yml]
indent_style = space
indent_size = 2